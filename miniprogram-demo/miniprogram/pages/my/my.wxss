/* pages/my/my.wxss */
.container{
  width:100%;
  min-height: 100%;
  display:flex;
  flex-direction: column;
  background-color: #fff;
}
.my-title{
  width: 100%;
  height: 360rpx;
  display: inline-flex;
  justify-content: space-between;
}
.my-title-left{
  display: flex;
  flex-direction: column;
}
.my-title-left image{
  width: 150rpx;
  height: 150rpx;
}
.my-title-left text{
  font-size: 50rpx;
  font-weight: 400;
}
.my-title-right{
  display: flex;
  flex-direction: column;
}
.dividing-line{
  width: 100%;
  height: 2rpx;
  background-color: black;
}
.my-list{
  width: 100%;
  height: 230rpx;
  display: inline-flex;
  justify-content: space-between;
}
.my-list-icon{
  width: 100rpx;
  height: 100rpx;
}
.my-list-text{
  width: 100%;
  height: 100%;
  font-size: 45rpx;
  font-weight: 400;
}
.my-list-arrow{
  width: 80rpx;
  height: 80rpx;
  right: 30rpx;
  position: absolute;
}